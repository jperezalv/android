<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#343434" />
            <stroke
                android:width="1dp"
                android:color="#371717" />
            <corners
                android:radius="200dp" />
            
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#443434"
                android:endColor="#483838"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#271717" />
            <corners
                android:radius="200dp" />
           
        </shape>
    </item>
</selector>